            //set the value to local variables
            string inputfolder = Dts.Variables["$Package::InputFolder"].Value.ToString();
            string archpath = Dts.Variables["$Package::ArchFolder"].Value.ToString() + "Sales_" + DateTime.Now.ToString("yyyyMMddHHmmss") + ".zip";

